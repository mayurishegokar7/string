#include <stdio.h>
#include <string.h>
int main()
{
   char s1[40],s2[40],i;
   printf("enter string 1:");
   fgets(s1,40,stdin);
   printf("enter string 2:");
   fgets(s2,40,stdin);
   i=strcmp(s1,s2);
   if(i==0)
     printf("strings are equal");
   else if (i>0)
     printf("string 1 is greater than string 2");
   else if (i<0)
     printf("string 2 is greater than string 1");
   return 0;
}
